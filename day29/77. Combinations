class Solution {
    void solve(int cur,int n, int k, vector<int>& ds, vector<vector<int>>& ans)
    {
        if(k==0) ans.push_back(ds);
        if(cur>n) return;
        for(int i=cur;i<=n;i++)
        {
            ds.push_back(i);
            solve(i+1, n, k-1, ds, ans);
            ds.pop_back();
        }
    }
public:
    vector<vector<int>> combine(int n, int k) {
        vector<vector<int>> ans;
        vector<int> ds;
        solve(1,n,k,ds,ans);
        return ans;
    }
};

static bool     _foo = ios::sync_with_stdio(false);
static ostream* _bar = cin.tie(NULL);
