class Solution {
public:
    int ans=0;
    void fonc(vector<vector<int>>& grid,int total,int x,int i,int j){
        if(i<0 || j<0 || i>=grid.size() || j>=grid[0].size()) return;
        if(grid[i][j]==INT_MIN) return;
        if(grid[i][j]==2 ){
            if(x==total)
                ans++;
            return ;
        }
        if(grid[i][j]==-1) return;
        grid[i][j]=INT_MIN;

        fonc(grid,total,x+1,i+1,j);
        fonc(grid,total,x+1,i-1,j);
        fonc(grid,total,x+1,i,j+1);
        fonc(grid,total,x+1,i,j-1);
        grid[i][j]=0;

    }
    int uniquePathsIII(vector<vector<int>>& grid) {
        int total=0;
        int x,y;
        for(int i=0;i<grid.size();i++){
            for(int j=0;j<grid[0].size();j++){
                if(grid[i][j]!=-1 && grid[i][j]!=2) total++;
                if(grid[i][j]==1){
                    x=i;
                    y=j;
                }
            }
        }
        fonc(grid,total,0,x,y);
        return ans;
    }
};
