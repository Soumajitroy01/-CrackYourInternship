class Solution {
public:
    vector<int> findDuplicates(vector<int>& nums) {
        //in place
        //use the numbers as reference to index and the index value*-1
        //while tarversing if we encounter a negative value then push it to the vector
        vector<int> res;
        for(int i=0;i<nums.size();i++)
        {
            if(nums[abs(nums[i])-1]<0) res.push_back(abs(nums[i]));
            else nums[abs(nums[i])-1] *= -1;
        }
        
        return res;
    }
};
