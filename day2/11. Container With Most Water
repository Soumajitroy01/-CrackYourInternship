class Solution {
public:
    int maxArea(vector<int>& height) {
        int lo=0, hi = height.size()-1, maxi=0;
        
        while(lo<hi)
        {
            int area = min(height[lo],height[hi])*(hi-lo);
            maxi = max(maxi,area);
            if(height[lo] < height[hi]) lo++;
            else hi--;
        }
        
        return maxi;
    }
};
