class Solution {
public:
    int subarraySum(vector<int>& a, int k) {
        unordered_map<int,int> mp;
        int n = a.size();
        int sum=0,res=0;
        mp[sum]++;
        for(int i=0;i<n;i++)
        {
            sum+=a[i];
            if(mp.find(sum-k)!=mp.end()) res+=mp[sum-k];
            mp[sum]++;
        }
        
        return res;
    }
};
