class Solution {
public:
    vector<int> rightSideView(TreeNode* root) {
        if(root==NULL) return {};
        vector<int> ans;
        queue<TreeNode *> q;
        q.push(root);
        while(!q.empty())
        {
            int size = q.size();
            
            for(int i=0; i<size; i++)
            {
                TreeNode *n = q.front();
                q.pop();
                
                if(n->left!=NULL)
                {
                    q.push(n->left);
                }
                
                if(n->right)
                {
                    q.push(n->right);
                }
                
                if(i==size-1) ans.push_back(n->val);
            }
        }
        
        return ans;
    }
};
