class Solution {
public:
    ListNode* removeNthFromEnd(ListNode* head, int n) {
        ListNode* temp = head;
        int count = 1;
        while(temp->next!=NULL){
            temp = temp->next;
            count++;
        }
        if(n > count) return NULL;
        if(n == count){
            head = head->next;
            return head;
        }
        int steps = count - n - 1;
        temp = head;
        while(steps--){
            temp = temp->next;
        }
        temp->next = temp->next->next;
        return head;
    }
};
