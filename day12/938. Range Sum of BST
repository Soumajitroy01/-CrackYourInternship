class Solution {
public:
    void FastIO()
    {
        ios_base::sync_with_stdio(0);
        cin.tie(0);
        //cout.tie(0);
    }
    int rangeSumBST(TreeNode* root, int low, int high) {
        FastIO();
        int sum=0;
        if(root==NULL) return 0;
        if(root->val>=low && root->val<=high)
            sum+= root->val + rangeSumBST(root->left,low,root->val) +
                  rangeSumBST(root->right,root->val,high);
        else if(root->val<low) sum+=rangeSumBST(root->right,low,high);
        else sum+= rangeSumBST(root->left,low,high);
        
        return sum;
    }
};
