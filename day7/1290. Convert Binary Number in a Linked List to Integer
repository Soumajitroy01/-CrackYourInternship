class Solution {
public:
    int getDecimalValue(ListNode* head) {
        int cnt=0,ans=0;
        ListNode *temp=head;
        while(temp!=NULL)
        {
            cnt++;
            temp=temp->next;
        }
        cnt--;
        while(head!=NULL)
        {
            ans+=head->val*pow(2,cnt--);
            head=head->next;
        }
        return ans;
    }
};
