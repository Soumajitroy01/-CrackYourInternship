long long  multiplyTwoLists (Node* l1, Node* l2)
{
    long long n1=0,n2=0;
    long long mod= 1000000007;
    while(l1!=NULL)
    {
        n1=((n1%mod)*10)%mod+l1->data;
        l1=l1->next;
    }
    while(l2!=NULL)
    {
        n2=((n2%mod)*10)%mod+l2->data;
        l2=l2->next;
    }
    
    return ((n1%mod)*(n2%mod))%mod;
}
