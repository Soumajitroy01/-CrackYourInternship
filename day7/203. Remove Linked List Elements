class Solution {
public:
    ListNode* removeElements(ListNode* head, int val) {
        ListNode *temp;
        while(head!=NULL and head->val==val)
        {
            head=head->next;
        }
        if(head==NULL) return head;
        temp=head;
        while(temp!=NULL and temp->next!=NULL)
        {
            if(temp->next->val==val)
            {
                temp->next=temp->next->next;
                continue;
            }
            temp=temp->next;
        }
        
        return head;
    }
};
