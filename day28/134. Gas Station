class Solution {
public:
    int canCompleteCircuit(vector<int>& gas, vector<int>& cost) {
        int check =0;
        int ans =0;
        int remaining =0;
        for(int i=0;i<gas.size();i++)
        {
            check += gas[i] - cost[i];
            remaining += gas[i] - cost[i];
            if(remaining <0)
            {
                remaining = 0;
                ans = i+1;
            }
        }
        if(check<0) return -1;
        return ans;
    }
};
