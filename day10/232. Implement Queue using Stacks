class MyQueue {
public:
    stack<int> s1,s2;
    MyQueue() {
        
    }
    
    void push(int x) {
        s1.push(x);
    }
    
    int pop() {
        int n=-1;
        if(!s2.empty()){
            n=s2.top();
            s2.pop();
        }
        else{
            while(!s1.empty()){
                s2.push(s1.top());
                s1.pop();
            }
            if(!s2.empty()){
                n=s2.top();
                s2.pop();
            }
        }
        return n;
    }
    
    int peek() {
        int n=-1;
        if(!s2.empty()){
            n=s2.top();
            
        }
        else{
            while(!s1.empty()){
                s2.push(s1.top());
                s1.pop();
            }
            if(!s2.empty()){
                n=s2.top();
            
            }
        }
        return n;
    }
    
    bool empty() {
        return s1.empty()&&s2.empty() ? true : false;
    }
};
