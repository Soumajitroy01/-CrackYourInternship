class Solution {
public:
    vector<int> ans;
    void traverse (TreeNode* root)
    {
        if(root==NULL) return;
        traverse(root->left);
        ans.push_back(root->val);
        traverse(root->right);
    }
    vector<int> inorderTraversal(TreeNode* root) {
        traverse(root);
        return ans;
    }
};
