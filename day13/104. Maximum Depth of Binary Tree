class Solution {
public:
    int max(TreeNode *root)
    {
        int lh,rh;
        if(root!=NULL)
        {
            lh=max(root->left);
            rh=max(root->right);
            if(lh>rh) return lh+1;
            else return rh+1;
        }
        return 0;
    }
    int maxDepth(TreeNode* root) {
        return max(root);
    }
};
