#include <bits/stdc++.h>
using namespace std;

void printduplicates(string s)
{
    unordered_map<char,int> mp;

    for(auto i:s)
        mp[i]++;

    for(auto i:mp)
        if(i.second>1)
            cout<<i.first<<" "<<i.second<<"\n";
}

int main()
{
    string s;
    cin>>s;
    printduplicates(s);
    return 0;
}
