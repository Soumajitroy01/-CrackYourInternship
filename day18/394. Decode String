class Solution {
public:
    string decodeString(string s) {
        stack<char> st;
        for(int i=0;i<s.length();i++)
        {
            if(s[i]!=']')
                st.push(s[i]);
            else
            {
                string substr="";
                while(st.top()!='[')
                {
                    substr = st.top()+substr;
                    st.pop();
                }
                st.pop();
                
                string k="";
                while(!st.empty() && isdigit(st.top()))
                {
                    k = st.top() + k;
                    st.pop();
                }
                
                int len=stoi(k);
                while(len--)
                {
                    for(int j=0;j<substr.length();j++)
                        st.push(substr[j]);
                }
            }
        }
        string ans="";
        while(!st.empty())
        {
            ans = st.top() + ans;
            st.pop();
        }
        
        return ans;
    }
};

static bool     _foo = ios::sync_with_stdio(false);
static ostream* _bar = cin.tie(NULL);
