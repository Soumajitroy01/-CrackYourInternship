class Solution {
//dp[t_i + 1] [0, t_i]
    long dp[1001]{1}; //dp initialized to [1, 0, 0, 0, ...]
    int m , n;
public:
    int numDistinct(string &s, string &t) {
        
        if((m = s.size()) < (n = t.size()))
            return 0;
        for(int s_i = 0; s_i < m; s_i++)
            for(int t_i = min(s_i, n - 1); t_i >= 0; t_i--){
                if(s[s_i] == t[t_i])
                    if((dp[t_i + 1] = dp[t_i] + dp[t_i + 1]) > INT_MAX)
                        dp[t_i + 1] = 0;
                else
                    dp[t_i + 1] = dp[t_i + 1];
            }
        return dp[n];
    }
};
