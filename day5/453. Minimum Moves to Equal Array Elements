class Solution {
public:
    int minMoves(vector<int>& arr) {
        int mini=arr[0];
        int n = arr.size();
        for(int i=1;i<n;i++)
            mini = min(mini,arr[i]);
        
        int ans=0;
        for(int i=0;i<n;i++)
            ans+=arr[i]-mini;
        
        return ans;
    }
};
