class Solution {
public:
    void solve(int n,int open,int close, string s, vector<string> &ans)
    {
        if(s.length()==n*2)
        {
            ans.push_back(s);
            return;
        }
        
        if(open<n)
            solve(n,open+1,close,s+'(',ans);
        
        if(open>close)
            solve(n,open,close+1,s+')',ans);
    }
    
    vector<string> generateParenthesis(int n) {  
        static const auto fast=[](){ios_base::sync_with_stdio(false);cin.tie(nullptr);cout.tie(nullptr);return nullptr;}();
        int open=0,close=0;
        vector<string> ans;
        string s="(";
        
        solve(n,open+1,close,s,ans);
        
        return ans;
    }
};
